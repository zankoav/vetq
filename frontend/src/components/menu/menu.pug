mixin menu
    .menu-wrapp
        ul.menu
            li.menu__item
                a(href="/") Главная	
            li.menu__item.menu__item-sub-menu
                a(href="/catalog.html") Каталог
                ul.menu__sub-menu
                    li.menu__item
                        a(href="/product.html") Продукт 1	
                    li.menu__item
                        a(href="/product.html") Продукт 2	
                    li.menu__item.menu__item_active
                        a(href="/product.html") Продукт 3	
                    li.menu__item
                        a(href="/product.html") Продукт 4	
            li.menu__item.menu__item_active
                a(href="/contacts.html") Контакты
        a.menu-wrapp__button(href="javascript:void(0);")
